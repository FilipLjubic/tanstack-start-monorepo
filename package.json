{
  "name": "tanstack-start-monorepo",
  "version": "1.0.0",
  "description": "TanStack Start monorepo with Better Auth, Drizzle, and Railway deployment",
  "scripts": {
    "dev": "pnpm --filter @starter/web dev",
    "test": "vitest run",
    "lint": "pnpm exec biome check --write",
    "catalogify": "pnpx codemod pnpm/catalog",
    "supabase": "pnpm --filter @starter/backend exec supabase"
  },
  "author": "FilipLjubic",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=22.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "dotenv-cli": "catalog:",
    "tsx": "catalog:",
    "vitest": "catalog:"
  }
}
