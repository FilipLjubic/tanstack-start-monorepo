{
  "name": "@starter/backend",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./*": "./src/*.ts",
    "./services/*": "./src/services/*.ts",
    "./services/auth/*": "./src/services/auth/*.ts",
    "./services/auth/user/*": "./src/services/auth/user/*.ts",
    "./types/*": "./src/types/*.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "scripts": {
    "better-auth:generate": "pnpx @better-auth/cli@1.3.34 generate",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:migrate:prod": "sh -c 'set -a && . ./.env.prod && drizzle-kit migrate'",
    "drizzle:push": "drizzle-kit push",
    "drizzle:pull": "drizzle-kit pull",
    "drizzle:check": "drizzle-kit check",
    "drizzle:up": "drizzle-kit up",
    "drizzle:studio": "drizzle-kit studio",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset --local",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@better-auth/cli": "catalog:",
    "@starter/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "drizzle-kit": "catalog:"
  },
  "dependencies": {
    "@starter/logger": "workspace:*",
    "@supabase/supabase-js": "catalog:",
    "better-auth": "catalog:",
    "date-fns": "catalog:",
    "dotenv": "catalog:",
    "drizzle-orm": "catalog:",
    "postgres": "catalog:",
    "uuid": "catalog:",
    "zod": "catalog:"
  }
}
